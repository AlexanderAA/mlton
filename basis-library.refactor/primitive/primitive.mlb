ann 
   "allowConstant true"
   "allowFFI true"
   "allowPrim true"
   "allowRebindEquals true"
   "deadCode true"
   "nonexhaustiveMatch warn"
   "redundantMatch warn"
   "sequenceNonUnit warn"
   "warnUnused false"
in
   prim-basis.mlb
   ann "forceUsed" in
      ../config/choose.sml
      ../config/c/$(TARGET_ARCH)-$(TARGET_OS)/c-types.sml
   end
   primitive.sml
   ann "forceUsed" in
      basis-ffi.sml
   end
end
