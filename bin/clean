#!/bin/sh

# This script removes all the junk files created when compiling with MLton's
# various -keep flags.

/bin/rm -f *.basis *.chunks *.const *.core-ml *.cps *.flat *.flow 	\
	*.inline *.mono *.o *.once-graph *.once-vars *.post-useless 	\
	*.raise-to-jump *.redundant *.reg *.rep *.sxml *.sxml.poly	\
	*.threshold[0-9] *.useless *.xml

/bin/rm -f *~ core mlmon.out
