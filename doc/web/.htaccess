# Indexes is necessary for the benchmark directory to be shown.
Options FollowSymLinks Indexes

# Suresh has a broken link that points to /MLton.
Redirect permanent /MLton http://www.mlton.org/

# ViewCVS
Redirect permanent /cvs http://cvs.mlton.org/cgi-bin/viewcvs.cgi/mlton/mlton
Redirect permanent /cvs/ http://cvs.mlton.org/cgi-bin/viewcvs.cgi/mlton/mlton

# Moved on 2003-07-07
Redirect permanent /download.html http://www.mlton.org/download/index.html

ErrorDocument 404 /not-found.html
